<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Max20Percent</fullName>
    <active>true</active>
    <description>할인율 20% 이상 수정되지 않게</description>
    <errorConditionFormula>Discount_Percent__c &gt; 0.20</errorConditionFormula>
    <errorDisplayField>Discount_Percent__c</errorDisplayField>
    <errorMessage>할인율은 최대 20% 입니다.</errorMessage>
</ValidationRule>
